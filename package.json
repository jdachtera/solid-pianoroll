{
  "version": "0.0.27",
  "name": "solid-pianoroll",
  "description": "Pianoroll UI Control for Solid JS apps",
  "license": "MIT",
  "author": "jdachtera",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jdachtera/solid-pianoroll.git"
  },
  "homepage": "https://github.com/jdachtera/solid-pianoroll#readme",
  "bugs": {
    "url": "https://github.com/jdachtera/solid-pianoroll/issues"
  },
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@7.22.0",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "solid": {
      "development": "./dist/dev.jsx",
      "import": "./dist/index.jsx"
    },
    "development": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/dev.js"
      },
      "require": "./dist/dev.cjs"
    },
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build-dev": "vite build dev",
    "test": "concurrently pnpm:test:*",
    "build": "tsup",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "pnpm up -Li",
    "typecheck": "tsc --noEmit"
  },
  "optionalDependencies": {
    "@tonejs/midi": "^2.0.28",
    "tone": "^14.7.77"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.5",
    "esbuild-css-modules-plugin": "^2.7.1",
    "esbuild-plugin-solid": "^0.5.0",
    "jsdom": "^21.1.0",
    "prettier": "2.8.3",
    "sass": "^1.57.1",
    "solid-js": "^1.6.9",
    "tsup": "^6.5.0",
    "tsup-preset-solid": "^0.1.5",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1",
    "vite": "^4.0.4",
    "vite-plugin-solid": "^2.5.0",
    "vitest": "^0.28.3"
  },
  "keywords": [
    "solid"
  ]
}
